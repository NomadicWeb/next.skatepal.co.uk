---
title: Contact
listed: true
published: true
---
{% extends 'index'%}
{% include 'header' %}

{% block 'head' %} 
%small.page-breadcrumb {{ page.title }} 
{% endblock %}

{% block 'main' %}
.row.content-padding
  .col-md-9
    {% for contact in contents.contact_info %}
    .col-md-4
      .content-block.contact-info
        %h3 {{ contact.name }}
        {{ contact.img.url | image_tag: 'class:img-responsive'}}
        %h4 {{ contact.email }}
        %h5 {{ contact.number }}

    {% endfor %}

  .col-md-3
    .col-md-12
      .content-block
        {% include 'contact_form' %}
{% endblock %}

{% include 'footer' %}
