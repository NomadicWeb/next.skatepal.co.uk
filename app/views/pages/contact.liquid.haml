---
title: Contact
listed: true
published: true
position: 6
---
{% extends 'index'%}

{% block 'carousel' %}
//do nothing
{% endblock %}

{% include 'header' %}

{% block 'head' %} 
%small.page-breadcrumb {{ page.title }} 
{% endblock %}

{% block 'main' %}
.row.content-padding
  .contact-center
    #grid
      {% for contact in contents.contact_info %}
      .item
        .content-block.contact-info
          {{ contact.img.url | image_tag: 'class:img-responsive'}}
          %p.c-name   {{ contact.name }}
          %p.c-email  {{ contact.email }}
          %p.c-number {{ contact.number }}
      {% endfor %}
      :javascript
        get_layout(20, 28);
{% endblock %}

{% include 'footer' %}
