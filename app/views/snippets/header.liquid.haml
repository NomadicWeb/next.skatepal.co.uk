!!!
%html{lang: "en"}
  %head
    %meta{content: "text/html; charset=UTF-8", "http-equiv" => "content-type"}
    %meta{content: "width=device-width, initial-scale=1, maximum-scale=1", name: "viewport"}
    %meta{charset: "utf-8"}
    %meta{name: "keywords", content: "{{ site.meta_keywords }} {{ page.meta_keywords }}"}
    %title {{ page.title }} - {{ site.name }}

    %link{"href" => "http://fonts.googleapis.com/css?family=Oswald:400,300,700", "rel" => "stylesheet", "type" => "text/css"}
    %link{"href" => "http://fonts.googleapis.com/css?family=Abel", "rel" => "stylesheet", "type" => "text/css"}

    /[if lt IE 9]
      <script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
    %script{"type" => "text/javascript", "src" => "https://maps.googleapis.com/maps/api/js?key=AIzaSyCfWhP-xURy2HzjXVsmSdW_icaNngybK48"}
    {{ 'application' | stylesheet_tag }}
    {{ 'font-awesome.min' | stylesheet_tag }}

    {{ 'jquery.js' | javascript_tag }}
    {{ 'bootstrap.min.js' | javascript_tag }}
    {{ 'isotope.js' | javascript_tag }}
    {{ 'imagesloaded.js' | javascript_tag }}
    {{ 'main.js' | javascript_tag }}

    {% inline_editor %}
